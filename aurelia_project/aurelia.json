{
  "name": "proximo-web",
  "type": "project:application",
  "paths": {
    "root": "src",
    "resources": "au",
    "elements": "au/elements",
    "attributes": "au/attributes",
    "valueConverters": "au/value-converters",
    "bindingBehaviors": "au/binding-behaviors"
  },
  "transpiler": {
    "id": "typescript",
    "fileExtension": ".ts"
  },
  "build": {
    "options": {
      "server": "dev",
      "extractCss": "prod",
      "coverage": false
    }
  },
  "platform": {
    "hmr": false,
    "open": false,
    "port": 8080,
    "host": "localhost",
    "output": "dist"
  },
  "packageManager": "yarn"
}
